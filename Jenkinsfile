pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh 'echo "build...."'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "test...."'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploying...."'
                sh 'echo "Deploying more...."'
            }
        }
    }
}
